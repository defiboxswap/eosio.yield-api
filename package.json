{
  "name": "tokenyield-api",
  "version": "1.0.0",
  "description": "tokenyield history processing logic and api",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-mysql": "^3.1.0",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.11.0",
    "egg-swagger-doc": "^2.3.2",
    "egg-validate": "^2.0.2",
    "moment": "^2.24.0",
    "decimal.js": "^10.3.1"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=tokenyield-api --port 5051",
